#include <iostream>
using namespace std;
main (){
	 int i;
	 float suhu [5];
	 
	 cout << "Masukkan 5 buah data suhu" <<endl;
	 for (i = 0; i < 5 ; i++){
		 cout << i+1 << " : ";
		 cin >> suhu [i];
	 }
	 cout << "data suhu yang anda masukkan : " <<endl;
	 for ( i = 0; i < 5; i++){
		 cout <<suhu[i] << endl;
	 }
 }
